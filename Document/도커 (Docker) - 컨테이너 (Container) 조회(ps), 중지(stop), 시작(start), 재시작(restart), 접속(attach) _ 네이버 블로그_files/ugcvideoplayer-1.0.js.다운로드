ugcVideoPlayer=function ugcVideoPlayer(h,j,i,g){var f={};f.videoId=h;f.key=j;f.width=i;f.height=g;f.properties={copyrighter:null,playerType:null,calllbackDomain:null,infoDisplay:null,titleDisplay:null,playCountDisplay:null,linkCountDisplay:null,writerDisplay:null,postDayDisplay:null,hasThumbnails:null,hasLink:null,hasFullScreen:null,hasLogo:null,hasRelativeMovie:null,sizeRatio:null,autoPlay:null,beginTime:null};f.getVideoIframeTag=function(){return videoIframeTag(h,j,i,g,f.properties)};return f};var videoIframeTag=function videoIframeTag(s,p,w,n,r){var o=null;var v=300;var x=169;var m=null;var u=function(a){return(!a||0===a.length||/^\s*$/.test(a))};var t=function(a){if(typeof a==="string"){a={value:a,outKeyMode:false}}return a};var m=t(p);if(u(s)||u(m.value)){return null}if(u(w)){w=v}if(u(n)){n=x}var q="<iframe frameborder='0' scrolling='no' width='"+w+"px' height='"+n+"px' src='"+window.location.protocol+"//serviceapi.nmv.naver.com/view/ugcPlayer.nhn?vid="+s;if(m.outKeyMode){q+="&outKey="+m.value}else{q+="&inKey="+m.value}if(r!=null){if(r.copyrighter!==undefined&&r.copyrighter!=null&&r.copyrighter!=""){q+="&copyrighter="+r.copyrighter}if(r.playerType!==undefined&&r.playerType!=null&&r.playerType!=""){q+="&pType="+r.playerType}if(r.callbackDomain!==undefined&&r.callbackDomain!=null&&r.callbackDomain!=""){q+="&callbackDomain="+r.callbackDomain}if(r.infoDisplay!==undefined&&r.infoDisplay!=null){q+="&infoDisplay="+r.infoDisplay}if(r.titleDisplay!==undefined&&r.titleDisplay!=null){q+="&titleDisplay="+r.titleDisplay}if(r.playCountDisplay!==undefined&&r.playCountDisplay!=null){q+="&playCountDisplay="+r.playCountDisplay}if(r.linkCountDisplay!==undefined&&r.linkCountDisplay!=null){q+="&linkCountDisplay="+r.linkCountDisplay}if(r.writerDisplay!==undefined&&r.writerDisplay!=null){q+="&writerDisplay="+r.writerDisplay}if(r.postDayDisplay!==undefined&&r.postDayDisplay!=null){q+="&postDayDisplay="+r.postDayDisplay}if(r.hasThumbnails!==undefined&&r.hasThumbnails!=null){q+="&hasThu